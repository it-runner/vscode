	"tokenColors": [
		{
			"scope": "string",
			"settings": {
			"foreground": "#7586bb"
		},
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#727881"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#c48bdf"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#6dccb7"
			}
		},
		{
			"name": "Class names",
			"scope": [
				"entity.name.type.class",
				"entity.name.type.class.js",
				"entity.name.type.class.ts",
				"entity.name.type.class.php",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#dbc925"
			}
		},
		{
			"name": "HTML Text",
			"scope": [
				"text.html.basic",
				"punctuation.definition.tag.html",
				"entity.name.tag.html"
			],
			"settings": {
				"foreground": "#c48bdf"
			}
		},
		{
			"name": "HTML Tag Brackets",
			"scope": [
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#c48bdf"
			}
		},
		{
			"name": "HTML Attribute Values",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.single.html",
				"punctuation.definition.string.begin.html",
				"punctuation.definition.string.end.html"
			],
			"settings": {
				"foreground": "#58a594"
			}
		},
		{
			"name": "CSS Class Selectors",
			"scope": "entity.other.attribute-name.class.css",
			"settings": {
				"foreground": "#c48bdf"
			}
		},
		{
			"name": "CSS ID Selectors",
			"scope": "entity.other.attribute-name.id.css",
			"settings": {
				"foreground": "#c48bdf"
			}
		},
		{
			"name": "CSS Universal Selector (*)",
			"scope": [
			  "entity.name.tag.wildcard.css",
			  "meta.selector.css"
			],
			"settings": {
			  "foreground": "#c48bdf"
			}
		},
		{
			"name": "CSS URLs",
			"scope": [
				"string.quoted.double.css",
				"string.quoted.single.css",
				"meta.property-value.css string.quoted",
				"support.function.url.css",
				"meta.function.url.css"
			],
			"settings": {
				"foreground": "#c48bdf"
			}
		},
		{
			"name": "JSON Values",
			"scope": [
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.structure.dictionary.json meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json meta.structure.dictionary.value.json constant.language.json",
				"meta.structure.dictionary.json meta.structure.dictionary.value.json constant.numeric.json"
			],
			"settings": {
				"foreground": "#c48bdf"
			}
		},
		{
			"name": "Markdown Headers",
			"scope": [
				"markup.heading.markdown",
				"punctuation.definition.heading.markdown",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#c48bdf",
			}
		},
		{
			"name": "Plain Text",
			"scope": [
			  "text.plain",
			  "text"
			],
			"settings": {
			  "foreground": "#7586bb"
			}
		},
		{
			"name": "Log Files",
			"scope": [
				"text.log",
				"source.log"
			],
			"settings": {
				"foreground": "#c6c1bd"
			}
		},
	]
}
