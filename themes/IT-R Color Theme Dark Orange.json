	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#727881"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#92acc2"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#e28871"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#cac5bb"
			}
		},
		{
			"name": "Class names",
			"scope": [
				"entity.name.type.class",
				"entity.name.type.class.js",
				"entity.name.type.class.ts",
				"entity.name.type.class.php",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#44c493"
			}
		},
		{
			"name": "HTML Text",
			"scope": [
				"text.html.basic",
				"punctuation.definition.tag.html",
				"entity.name.tag.html"
			],
			"settings": {
				"foreground": "#e68e1a"
			}
		},
		{
			"name": "HTML Tag Brackets",
			"scope": [
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#e68e1a"
			}
		},
		{
			"name": "HTML Attribute Values",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.single.html",
				"punctuation.definition.string.begin.html",
				"punctuation.definition.string.end.html"
			],
			"settings": {
				"foreground": "#92acc2"
			}
		},
		{
			"name": "HTML Attribute Names",
			"scope": [
				"entity.other.attribute-name.html",
				"punctuation.separator.key-value.html"
			],
			"settings": {
				"foreground": "#f1c66a"
			}
		},
		{
			"name": "HTML Text Content",
			"scope": [
				"text.html.derivative",
				"meta.tag.inline.any.html",
				"meta.tag.block.any.html"
			],
			"settings": {
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Markdown Text",
			"scope": [
				"text.html.markdown",
				"punctuation.definition.list.begin.markdown"
			],
			"settings": {
				"foreground": "#d1c4ab"
			}
		},
		{
			"name": "CSS Class Selectors",
			"scope": "entity.other.attribute-name.class.css",
			"settings": {
				"foreground": "#92acc2"
			}
		},
		{
			"name": "CSS ID Selectors",
			"scope": "entity.other.attribute-name.id.css",
			"settings": {
				"foreground": "#92acc2"
			}
		},
		{
			"name": "CSS Universal Selector (*)",
			"scope": [
			  "entity.name.tag.wildcard.css",
			  "meta.selector.css"
			],
			"settings": {
			  "foreground": "#92acc2"
			}
		},
		{
			"name": "CSS Property Values",
			"scope": [
				"support.constant.property-value.css",
				"support.constant.font-name.css",
				"support.constant.color.w3c-standard-color-name.css",
				"constant.numeric.css",
				"constant.other.color.rgb-value.css",
				"constant.other.color.hex.css",
				"constant.other.color.hsl.css"
			],
			"settings": {
				"foreground": "#e28871"
			}
		},
		{
			"name": "CSS URLs",
			"scope": [
				"string.quoted.double.css",
				"string.quoted.single.css",
				"meta.property-value.css string.quoted",
				"support.function.url.css",
				"meta.function.url.css"
			],
			"settings": {
				"foreground": "#808f9b"
			}
		},
		{
			"name": "JSON Values",
			"scope": [
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.structure.dictionary.json meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json meta.structure.dictionary.value.json constant.language.json",
				"meta.structure.dictionary.json meta.structure.dictionary.value.json constant.numeric.json"
			],
			"settings": {
				"foreground": "#92acc2"
			}
		},
		{
			"name": "Markdown Headers",
			"scope": [
				"markup.heading.markdown",
				"punctuation.definition.heading.markdown",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#808f9b",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Plain Text",
			"scope": [
			  "text.plain",
			  "text"
			],
			"settings": {
			  "foreground": "#ff6600"
			}
		},
		{
			"name": "Log Files",
			"scope": [
				"text.log",
				"source.log"
			],
			"settings": {
				"foreground": "#c6c1bd"
			}
		}
	]
}
